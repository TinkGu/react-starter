{
  "name": "starter",
  "version": "1.0.0",
  "description": "react-starter",
  "main": "src/app.tsx",
  "scripts": {
    "dll:dev": "cross-env cross-env NODE_ENV=development webpack --config build/webpack.config.dll.js --mode development",
    "dll": "cross-env webpack --config build/webpack.config.dll.js --mode production",
    "start": "cross-env BUILD_TYPE=test NODE_ENV=development webpack-dev-server --hot --config ./build/webpack.config.dev.js --progress --colors",
    "release:dev": "cross-env NODE_ENV=production BUILD_TYPE=test webpack --config build/webpack.config.prod.js",
    "release:uat": "cross-env NODE_ENV=production BUILD_TYPE=uat webpack --config build/webpack.config.prod.js",
    "release": "cross-env NODE_ENV=production BUILD_TYPE=prod webpack --config build/webpack.config.prod.js",
    "analyzer": "cross-env NODE_ENV=production webpack --config build/webpack.config.analyzer.js",
    "lint": "npm run lint:src",
    "lint:ts": "tsc -p tsconfig.json --noEmit",
    "lint:src": "node_modules/.bin/eslint src --quiet --ext ts,tsx,js,jsx",
    "fat": "npm run dll && npm run release:dev",
    "uat": "npm run dll && npm run release:uat",
    "pro": "npm run dll && npm run release"
  },
  "author": "tlwrite@126.com",
  "license": "ISC",
